{
  "name": "safeher",
  "version": "2.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint --fix",
    "format": "prettier -w .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "semi": true,
    "trailingComma": "none",
    "tabWidth": 2,
    "singleQuote": false,
    "importOrder": [
      "^components/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "root": true,
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "@bacons/expo-router-top-tabs": "^0.1.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@tamagui-extras/core": "^1.5.0",
    "@tamagui-extras/form": "^1.7.0",
    "@tamagui/animations-react-native": "^1.75.7",
    "@tamagui/babel-plugin": "^1.75.7",
    "@tamagui/config": "^1.75.7",
    "@tamagui/font-inter": "^1.75.7",
    "@tamagui/lucide-icons": "^1.75.7",
    "@tamagui/themes": "^1.75.7",
    "axios": "^1.6.0",
    "expo": "49.0.16",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-location": "~16.1.0",
    "expo-router": "2.0.10",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-maps": "1.7.1",
    "react-native-parsed-text": "^0.0.22",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-typing-animation": "^0.1.7",
    "tamagui": "^1.75.7",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/react": "~18.2.33",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
